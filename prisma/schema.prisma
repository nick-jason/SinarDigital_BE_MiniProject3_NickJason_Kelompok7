// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
  // Generated client needs to be located where @prisma/client expects it.
  // Place generated files under node_modules/@prisma/client/.prisma/client
  output   = "../node_modules/@prisma/client/.prisma/client"
}

datasource db {
  provider = "mysql"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}